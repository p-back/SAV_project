<Window x:Class="MotionTest.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MotionTest"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800" Background="#FF1F1E1E">
    <Grid>
        <StackPanel Orientation="Horizontal">
            <TextBlock Name="output" Margin="10" Foreground="Black" Background="White" MinWidth="500"/>
            <ListBox Margin="30,10,10,10" MinWidth="200" Panel.ZIndex="51"/>
        </StackPanel>
    </Grid>
</Window>
